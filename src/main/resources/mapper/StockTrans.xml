<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.demo.myspring.utils.stock.Dao.StockTransDao">

    <select id="queryAccountById"   resultType="com.demo.myspring.entity.Account">
        select * from Account
        where id = ${id}
    </select>

    <select id="updateAccountById"   parameterType ="com.demo.myspring.entity.Account" resultType="int" >
        update account set money = #{account.money} where id = #{account.id}
    </select>

</mapper>
